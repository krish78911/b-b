{% block body %}
    
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-lg-6">
                <div class="row" id="admin-bookings">
                    <div class="col-lg-12">
                        <div class="adminHeading">MANAGE BOOKINGS</div>
                    </div>
                    <div class="col-lg-12 admin-oneBooking" id="adminBooking">
                        {% include 'admin/bookings.html.twig' with {'bookings': bookings} %}
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-lg-6">
                <div class="row" id="admin-rooms">
                    <div class="col-lg-12">
                        <div class="adminHeading">MANAGE ROOMS</div>
                    </div>
                    <div class="col-lg-12 admin-oneBooking" id="adminRoom">
                        {% include 'admin/rooms.html.twig' with {'rooms': rooms} %}
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-12">
                <div class="row" id="admin-rooms">
                    <div class="col-lg-12">
                        <div class="adminHeading">ADD ROOM</div>
                    </div>
                    <div class="col-lg-12 admin-oneBooking" id="adminAddRoom">
                        {% include 'admin/addroom.html.twig' %}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}